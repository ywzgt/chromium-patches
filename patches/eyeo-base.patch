+#ifndef COMPONENTS_ADBLOCK_CORE_SITEKEY_STORAGE_H_
+#define COMPONENTS_ADBLOCK_CORE_SITEKEY_STORAGE_H_
+
+#include <string>
+
+#include "absl/types/optional.h"
