+
+absl::optional<base::StringPiece> ExtractRegexFilterFromPattern(
+    base::StringPiece filter_pattern) {
+  if (!(filter_pattern.size() > 2 && filter_pattern.front() == '/' &&
+        filter_pattern.back() == '/')) {
+    return absl::nullopt;
+  }
+  return filter_pattern.substr(1, filter_pattern.size() - 2);
+}
